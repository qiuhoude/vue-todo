html, body
  margin 0
  padding 0
  width 100%
  height 100%

body
  background-image url("../images/bgblue.jpg")
  background-size cover
  font 14px
  color #333333
  font-weight 500

*
  box-sizing border-box

.fade-enter-active, .fade-leave-active
  transition: opacity .5s

.fade-enter, .fade-leave-to
  opacity: 0
